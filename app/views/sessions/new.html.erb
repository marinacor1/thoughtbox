<div class="login-page">
  <center><h1>Log In</h1></center>
<center><div class="sign-text">
  <%= form_for (:session) do |f| %>
    <%= f.label :email, "Email:", class: "label-new" %>
    <%=  f.text_field :email %>
    <%= f.label :password, "Password:", class: "label-password" %>
    <%=  f.text_field :password %>
    <%= f.submit "Log In" %>
  <% end %>
</div></center>
</div>
