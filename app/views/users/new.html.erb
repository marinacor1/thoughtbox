<h1>Create an Account</h1>

<%= form_for @user do |f| %>
  <%= f.label "Email:" %>
  <%=  f.text_field :email %>
  <%= f.label "Password:" %>
  <%=  f.text_field :password %>
  <%= f.label "Password Confirmation:" %>
  <%=  f.text_field :password_confirmation %>
  <%= f.submit "Create Account" %>
<% end %>
